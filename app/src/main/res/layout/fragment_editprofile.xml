            android:src="@drawable/down_arrow"
            app:layout_constraintBottom_toBottomOf="@id/txtState"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/txtState"
            app:layout_constraintTop_toTopOf="@id/txtState" />


        <View
            android:id="@+id/viewState"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginHorizontal="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@color/HomeThemeColor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/txtState" />

        <TextView
            android:id="@+id/tvState"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_15sdp"
            android:fontFamily="@font/public_sans_regular"
            android:text=""
            android:textColor="@color/error_color"
            android:textSize="@dimen/_10sdp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/viewState" />

        // state

        <TextView
            android:id="@+id/txtCity"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:fontFamily="@font/public_sans_regular"
            android:hint="@string/select_your_city"
            android:text=""
            android:textColor="@color/Heading_Color"
            android:textSize="@dimen/_11sdp"
            android:paddingVertical="@dimen/_5sdp"
            app:layout_constraintEnd_toStartOf="@id/icon1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvState" />

        <ImageView
            android:id="@+id/icon2"
            android:layout_width="@dimen/_10sdp"
            android:layout_height="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            android:src="@drawable/down_arrow"
            app:layout_constraintBottom_toBottomOf="@id/txtCity"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/txtCity"
            app:layout_constraintTop_toTopOf="@id/txtCity" />

        <View
            android:id="@+id/viewCity"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginHorizontal="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@color/HomeThemeColor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/txtCity" />

        <TextView
            android:id="@+id/tvCity"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_15sdp"
            android:fontFamily="@font/public_sans_regular"
            android:text=""
            android:textColor="@color/error_color"
            android:textSize="@dimen/_10sdp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/viewCity"/>



        <LinearLayout
            android:id="@+id/llUploadDocument"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/_5sdp"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tvCity">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/parentLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/btnUploadDocument"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/upload_documents"
                    android:visibility="visible"
                    android:textAllCaps="false"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white"
                    android:background="@null"
                    android:layout_marginStart="@dimen/_15sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="@dimen/_10sdp"/>

                <TextView
                    android:id="@+id/txtEditUploadedDocument"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_11sdp"
                    android:textColor="@color/HomeThemeColor"
                    tools:text=""
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    app:layout_constraintStart_toEndOf="@id/btnUploadDocument"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@id/btnUploadDocument"
                    app:layout_constraintBottom_toBottomOf="@id/btnUploadDocument"/>

                <View
                    android:id="@+id/viewUploadDocument"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginHorizontal="@dimen/_15sdp"
                    android:background="@color/HomeThemeColor"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/btnUploadDocument" />

                <TextView
                    android:id="@+id/tvUploadDocument"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/error_color"
                    android:textSize="@dimen/_10sdp"
                    android:text=""
                    android:layout_marginHorizontal="@dimen/_15sdp"
                    android:visibility="gone"
                    android:fontFamily="@font/public_sans_regular"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/viewUploadDocument"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>


